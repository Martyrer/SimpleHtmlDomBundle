{
    "name": "martyrer/simple-html-dom-bundle",
    "description": "Bundle around Simple HTML DOM library",
    "type": "symfony-bundle",
    "keywords": ["html", "dom", "php"],
    "homepage": "https://github.com/Martyrer/SimpleHtmlDomBundle",
    "license": "MIT",
    "authors": [
        {"name": "Dmitry Shumilin", "email": "zveryga.ru@gmail.com"}
    ],
    "require": {
        "php": "^7.1",
        "symfony/framework-bundle": "~3.4|~4.0",
        "roave/security-advisories": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "7.0.*",
        "symfony/browser-kit": "~3.4|~4.0"
    },
    "autoload": {
      "psr-4": { "Martyrer\\SimpleHtmlDomBundle\\": "src" }
    },
    "autoload-dev": {
      "psr-4": { "Martyrer\\SimpleHtmlDomBundle\\Tests\\": "Tests" }
    }
}
