{
    "name": "martyrer/simple-html-dom-bundle",
    "description": "Bundle around Simple HTML DOM library",
    "type": "symfony-bundle",
    "keywords": ["html", "dom", "php"],
    "homepage": "https://github.com/Martyrer/SimpleHtmlDomBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Shumilin",
            "email": "zveryga.ru@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/finder": "^3.4|^4.0",
        "symfony/yaml": "^3.4|^4.0",
        "symfony/form": "^3.4|^4.0",
        "symfony/browser-kit": "^3.4|^4.0",
        "symfony/translation": "^3.4|^4.0",
        "symfony/validator": "^3.4|^4.0",
        "symfony/security-csrf": "^3.4|^4.0",
        "doctrine/annotations": "^1.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.*"
