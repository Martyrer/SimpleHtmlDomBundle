    "type": "symfony-bundle",
    "keywords": ["html", "dom", "php"],
    "homepage": "https://github.com/Martyrer/SimpleHtmlDomBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Shumilin",
            "email": "zveryga.ru@gmail.com"
        }
    ],
    "require": {
